{
  "name": "pb/foodconfig",
  "description": "FoodConfig - wyszukiwarka przepisów oparta na preferencjach użytkownika",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "foodconfig"
  ],
  "homepage": "http://foodconfig.pl/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3 || ^8.0",
    "laminas/laminas-component-installer": "^2.5.1",
    "laminas/laminas-mvc": "^3.0.1",
    "composer-plugin-api": "^2.0",
    "laminas/laminas-development-mode": "^3.0",
    "laminas/laminas-cache": "^2.7.1",
    "laminas/laminas-db": "^2.8.1",
    "laminas/laminas-mvc-form": "^1.0",
    "laminas/laminas-json": "^3.0",
    "laminas/laminas-log": "^2.9",
    "laminas/laminas-session": "^2.7.1",
    "laminas/laminas-navigation": "^2.9",
    "laminas/laminas-paginator": "^2.8",
    "alwayslearn/zend-generate-qr-code": "@dev",
    "laminas/laminas-permissions-rbac": "^3.0",
    "doctrine/migrations": "^2.0",
    "doctrine/data-fixtures": "^1.3",
    "doctrine/orm": "^2.6",
    "doctrine/dbal": "2.12.1",
    "doctrine/doctrine-orm-module": "^2.1",
    "laminas/laminas-crypt": "^3.3",
    "laminas/laminas-view": "^2.11",
    "laminas/laminas-mail": "^2.10",
    "laminas/laminas-captcha": "^2.8",
    "laminas/laminas-permissions-acl": "^2.7",
    "laminas/laminas-mvc-plugin-flashmessenger": "^1.1",
    "laminas/laminas-recaptcha": "^3.2",
    "laminas/laminas-dependency-plugin": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "FoodConfig\\": "module/FoodConfig/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FoodConfigTest\\": "module/FoodConfig/test/"
    }
  },
  "extra": [],
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "development-disable": "laminas-development-mode disable",
    "development-enable": "laminas-development-mode enable",
    "development-status": "laminas-development-mode status",
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  },
  "require-dev": {
    "laminas/laminas-developer-tools": "^1.1.0",
    "laminas/laminas-test": "^3.0.1"
  },
  "config": {
    "allow-plugins": {
      "laminas/laminas-dependency-plugin": true,
      "laminas/laminas-component-installer": true
    }
  }
}
